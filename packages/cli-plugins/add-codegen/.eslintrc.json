{
	"extends": ["../../../.eslintrc.json", "oclif", "oclif-typescript", "prettier"],
	"rules": {
		"perfectionist/sort-objects": "off",
		"import/no-unresolved": "off",
		"@typescript-eslint/consistent-type-imports": "off",
